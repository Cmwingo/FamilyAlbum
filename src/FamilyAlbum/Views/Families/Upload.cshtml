
<form enctype="multipart/form-data" id="family-image" name="family-image" asp-action="Upload" method="post">
    <div class="form-group">
        <div class="col-md-10">
            <p>Upload a file using this form:</p>
            <input type="file" name="file" id="file"/>
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-10">
            <input type="submit" value="Upload" />
        </div>
    </div>
</form>