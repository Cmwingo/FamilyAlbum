
<form method="post" enctype="multipart/form-data" class="new-file" asp-action="Upload">
    <div class="form-group">
        <div class="col-md-10">
            <p>Upload a file using this form:</p>
            <input type="file" name="file" />
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-10">
            <input type="submit" value="Upload" />
        </div>
    </div>
</form>